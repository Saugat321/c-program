 include<stdio.h>
 
   int main()
   {
      int i,sum=0,n;
      
      printf("Enter any number: \n");
      scanf("%d",&n);
      
      for i=0;i<=n;i++
      {
     
      sum+=i*i;
      
      printf("The sum is: %d %d : \n",sum,i);
      
      
      
      
      
      
      }
      
      
      return 0;
      }
      
      
